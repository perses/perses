            "collapse": {
              "open": false
            }
          },
          "items": [{
            "x": 0,
            "y": 2,
            "width": 12,
            "height": 10,
            "content": {
              "$ref": "#/spec/panels/1_0"
            }
          }]
        }
      }
    ]
  }
