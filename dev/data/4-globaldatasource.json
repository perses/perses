[
  {
    "kind": "GlobalDatasource",
    "metadata": {
      "name": "PrometheusLocal"
    },
    "spec": {
      "default": true,
      "plugin": {
        "kind": "PrometheusDatasource",
        "spec": {
          "proxy": {
            "kind": "HTTPProxy",
            "spec": {
              "url": "http://localhost:9090"
            }
          }
        }
      }
    }
  },
  {
    "kind": "GlobalDatasource",
    "metadata": {
      "name": "TempoLocal"
    },
    "spec": {
      "default": true,
      "plugin": {
        "kind": "TempoDatasource",
        "spec": {
          "proxy": {
            "kind": "HTTPProxy",
            "spec": {
              "url": "http://localhost:3200"
            }
          }
        }
      }
    }
  }
]
