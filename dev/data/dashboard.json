[
  {
    "kind": "Dashboard",
    "metadata": {
      "name": "SimpleLineChart",
      "project": "perses"
    },
    "spec": {
      "datasource": "PrometheusDemo",
      "duration": "6h",
      "sections": [
        {
          "name": "mySection",
          "open": true,
          "panels": [
            {
              "name": "myGraphPanel",
              "kind": "LineChart",
              "chart": {
                "lines": [
                  {
                    "expr": "up"
                  }
                ]
              }
            }
          ]
        },
        {
          "name": "mySection2",
          "open": true,
          "panels": [
            {
              "name": "myGraphPanel",
              "kind": "LineChart",
              "chart": {
                "lines": [
                  {
                    "expr": "up"
                  }
                ]
              }
            },
            {
              "name": "myGraphPanel",
              "kind": "LineChart",
              "chart": {
                "lines": [
                  {
                    "expr": "up"
                  },
                  {
                    "expr": "up"
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  }
]
