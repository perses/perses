{
  "extends": "../tsconfig.web.json",
  "include": ["src"],
  "compilerOptions": {
    "sourceMap": true,
    "noEmit": true
  },
  // For webpack builds that use ts-node to compile the configs
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "esModuleInterop": true
    }
  }
}
