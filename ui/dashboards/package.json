{
  "name": "@perses-dev/dashboards",
  "version": "0.21.0",
  "description": "The dashboards feature in Perses",
  "license": "Apache-2.0",
  "homepage": "https://github.com/perses/perses/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perses/perses.git"
  },
  "bugs": {
    "url": "https://github.com/perses/perses/issues"
  },
  "module": "dist/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "concurrently \"npm:build:*\"",
    "build:cjs": "swc ./src -d dist/cjs --config-file ../.cjs.swcrc",
    "build:esm": "swc ./src -d dist --config-file ../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --declaration --preserveWatchOutput",
    "start": "concurrently -P \"npm:build:* -- {*}\" -- --watch",
    "test": "TZ=UTC jest",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@perses-dev/components": "0.21.0",
    "@perses-dev/core": "0.21.0",
    "@perses-dev/plugin-system": "0.21.0",
    "@types/react-grid-layout": "^1.3.2",
    "date-fns": "^2.28.0",
    "immer": "^9.0.15",
    "mdi-material-ui": "^7.4.0",
    "react-grid-layout": "^1.3.4",
    "react-intersection-observer": "^9.4.0",
    "use-immer": "^0.7.0",
    "use-query-params": "^2.1.1",
    "use-resize-observer": "^9.0.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "intersection-observer": "^0.12.2"
  },
  "peerDependencies": {
    "@mui/material": "^5.10.0",
    "@tanstack/react-query": "^4.7.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "files": [
    "dist"
  ]
}
