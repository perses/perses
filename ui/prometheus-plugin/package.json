{
  "name": "@perses-dev/prometheus-plugin",
  "version": "0.1.0",
  "description": "Prometheus plugin for Perses",
  "license": "Apache-2.0",
  "homepage": "https://github.com/perses/perses/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perses/perses.git"
  },
  "bugs": {
    "url": "https://github.com/perses/perses/issues"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc",
    "test": "echo 'no test to run' && exit 0",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@lezer/lr": "^0.15.8",
    "@perses-dev/core": "^0.1.0",
    "@perses-dev/plugin-system": "^0.1.0",
    "date-fns": "^2.28.0",
    "lezer-promql": "^0.22.0",
    "react-query": "^3.34.16"
  },
  "devDependencies": {
    "react": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "perses": {
    "kind": "Plugin",
    "metadata": {
      "name": "Prometheus"
    },
    "spec": {
      "supported_kinds": {
        "PrometheusLabelNames": "Variable",
        "PrometheusLabelValues": "Variable",
        "Interval": "Variable",
        "PrometheusGraphQuery": "GraphQuery"
      },
      "plugin_module_path": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
