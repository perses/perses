{
  "name": "@perses-dev/panels-plugin",
  "version": "0.26.1",
  "description": "Core panels plugin for Perses",
  "license": "Apache-2.0",
  "homepage": "https://github.com/perses/perses/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/perses/perses.git"
  },
  "bugs": {
    "url": "https://github.com/perses/perses/issues"
  },
  "module": "dist/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist/",
    "build": "concurrently \"npm:build:*\"",
    "build:cjs": "swc ./src -d dist/cjs --config-file ../.cjs.swcrc",
    "build:esm": "swc ./src -d dist --config-file ../.swcrc",
    "build:types": "tsc --project tsconfig.build.json",
    "type-check": "tsc --noEmit",
    "start": "concurrently -P \"npm:build:* -- {*}\" -- --watch",
    "test": "TZ=UTC jest",
    "test:watch": "TZ=UTC jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "dependencies": {
    "@perses-dev/components": "0.26.1",
    "@perses-dev/core": "0.26.1",
    "@perses-dev/plugin-system": "0.26.1",
    "color-hash": "^2.0.2",
    "date-fns": "^2.28.0",
    "dompurify": "^2.4.0",
    "echarts": "^5.4.1",
    "immer": "^9.0.15",
    "lodash-es": "^4.17.21",
    "marked": "^4.2.12"
  },
  "peerDependencies": {
    "@mui/material": "^5.10.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "files": [
    "dist",
    "plugin.json"
  ],
  "devDependencies": {
    "@types/color-hash": "^1.0.2",
    "@types/dompurify": "^2.3.4",
    "@types/marked": "^4.0.7"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  }
}
